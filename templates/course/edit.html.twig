{% extends 'base.html.twig' %}

{% block title %}Modidier une annonce
{% endblock %}

{% form_theme form 'course/_collection.html.twig' %}

{% block body %}
	<div class="container">
		<h1>Modifier l'annonce :
			{{course.title}}</h1>

		{{ form_start(form) }}

		{{ form_widget(form) }}

		<button type="submit" class="btn btn-primary">Enregistrer les modifications</button>

		{{ form_end(form) }}
	</div>
{% endblock %}


{% block javascripts %}

	<script src="/js/script.js"></script>

{% endblock %}
